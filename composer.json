{
    "name": "maileryio/mailery-icon-material",
    "type": "library",
    "description": "Material Design Icons Pack for Mailery Platform",
    "keywords": [
        "mail",
        "mailer",
        "icon",
        "material-design"
    ],
    "homepage": "https://github.com/maileryio/mailery-icon-material",
    "license": "BSD-3-Clause",
    "support": {
        "source": "https://github.com/maileryio/mailery-icon-material",
        "issues": "https://github.com/maileryio/mailery-icon-material/issues",
        "wiki": "https://github.com/maileryio/mailery-icon-material/wiki"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": "^7.4",
        "foxy/foxy": "^1.0",
        "maileryio/mailery-icon": "dev-master",
        "yiisoft/arrays": "^1.0",
        "yiisoft/assets": "^1.0",
        "yiisoft/html": "^2.0"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^2.16",
        "phan/phan": "^2.4",
        "phpunit/phpunit": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "Mailery\\Icon\\Material\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Mailery\\Icon\\Material\\Tests\\": "tests"
        }
    },
    "scripts": {
        "lint": "php-cs-fixer fix --diff --dry-run",
        "fix": "php-cs-fixer fix",
        "phan": "phan --progress-bar -o analysis.txt"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        },
        "config-plugin": {
            "web": "web.php"
        },
        "config-plugin-options": {
            "source-directory": "config"
        }
    },
    "config": {
        "sort-packages": true
    }
}
